{
  "aws_access_key": "$AWS_ACCESS_KEY_ID",
  "aws_secret_key": "$AWS_SECRET_ACCESS",
  "aws_region": "$AWS_REGION",
  "vpc_name": "default",
  "ec2_instance_name": "$NAME",
  "ec2_instance_type": "m5.xlarge",
  "ec2_instance_public": true,
  "ec2_keypair": "$AWS_USER",
  "ec2_user_data": "terraform-provision.sh",
  "ec2_root_block_device_size": 15,
  "tags": {
    "category": "$NAME"
  }
}
