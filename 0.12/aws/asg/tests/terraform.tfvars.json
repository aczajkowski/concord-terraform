{
  "aws_access_key": "$AWS_ACCESS_KEY_ID",
  "aws_secret_key": "$AWS_SECRET_ACCESS",
  "aws_region": "$AWS_REGION",
  "vpc_name": "default",
  "asg_name": "$NAME",
  "asg_min_capacity": "1",
  "asg_max_capacity": "1",
  "instance_profile_role_name": "$NAME",
  "instance_profile_role_arn": "arn:aws:iam::aws:policy/AdministratorAccess",
  "launch_template_name": "$NAME",
  "launch_template_instance_type": "m5.xlarge",
  "launch_template_instance_profile": "$NAME",
  "launch_template_keypair": "$AWS_KEYPAIR",
  "launch_template_user_data": "terraform-provision.sh"
}
